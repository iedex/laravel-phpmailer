{
  "name": "iedex/laravel-phpmailer",
  "type": "project",
  "description": "PHPMailer transport for Laravel",
  "minimum-stability": "dev",
  "keywords": [
    "PHP",
    "PHPMailer",
    "Laravel",
    "transport"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Artem Parkhomov",
      "email": "parkhomov@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0|^9.0|^10.0",
    "phpmailer/phpmailer": "^6.6"
  },
  "autoload": {
    "psr-4": {
      "LaravelPHPMailer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelPHPMailer\\PHPMailerServiceProvider"
      ]
    }
  }
}
